[global_config]
[keybindings]
  move_tab_left = <Primary><Shift>Tab
  move_tab_right = <Primary>Tab
  new_tab = <Super>t
  new_window = <Super>n
  next_tab = <Primary>Tab
  split_horiz = <Primary>d
  split_vert = <Primary><Shift>d
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    scrollback_infinite = True
